{
	"name": "Spooky Website Lens",
	"version": "0.1",
	"description": "Spookify any website!",
	"permissions": ["tabs", "http://*/*", "https://*/*", "file:///*/*"],
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*", "file:///*/*"],
			"js": ["/content_script.js", "popup.js", "jquery-3.5.1.min.js"],
			"css": ["/style.css"]
		}
	],
	"browser_action": {
		"default_icon": "images/ghost-icon128.png",
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
		"images/*.png"
	],
    "icons": {
      "128": "images/ghost-icon128.png"
    },
    "manifest_version": 2
  }